UnlitGeneric
{
	"$basetexture" "vgui/hud/icon_silver_medal"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1
Proxies
{
AnimatedTexture
{ 
animatedTextureVar $basetexture 
animatedTextureFrameNumVar $frame
animatedTextureFrameRate 25 //this is the FPS that the texture will animate at
}
}
}

"UnlitGeneric"
{
    "$basetexture" "vgui/replay/thumbnails/radar/numbers"
    "$translucent" 1
    "$vertexcolor" 1
    "$no_fullbright" 1
    "$ignorez" 1
	
    $pos "[0.0 0.0 0.0]"
	$zero 0
	$five 5
    $size 256

	$posX .0
    $newpos .0
    $finalpos .0

    $offsetX 2378
    $totalX 5398

    $view   "0.0"

    // bottom left  -- adding 50 
    // -2438 -3158

    // top right
    // 2960 2115
    
    Proxies {
        PlayerProximity {
            scale 1
            resultVar $view
        }
        TextureTransform {
            translateVar $view
            resultVar $basetexturetransform
        }
    }
}
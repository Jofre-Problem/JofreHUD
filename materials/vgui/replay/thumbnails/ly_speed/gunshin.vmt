UnlitGeneric
{
"$baseTexture" "vgui/replay/thumbnails/Ly_speed\gunshin"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1

	Proxies
	{
		AnimatedTexture
		{
			animatedTextureVar "$basetexture"
			animatedTextureFrameNumVar "$frame"
			animatedTextureFrameRate "0.2"
		}
	}

}

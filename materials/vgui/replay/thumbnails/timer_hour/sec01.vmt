UnlitGeneric
{
"$baseTexture" "vgui\replay/thumbnails/timer_hour\digits"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1
$curtime 0
$const 60
$curtime2 0
$curtime3 0
$frame 1
$mins 0
$secs2 0
$secs3 0
$secs4 0
$secs 0
$const2 10

Proxies
{
   CurrentTime
    {
        "resultVar" "$curtime"
    }
  
   
   Divide
   {
        "srcVar1" "$curtime"
        "srcVar2" "$const"
        "resultVar" "$curtime2"
   }

   Int
   {
        "srcVar1" "$curtime2"
        "resultVar" "$mins"
   }

   Multiply
   {
        "srcVar1" "$mins"
        "srcVar2" "$const"
        "resultVar" "$curtime3"
   }
    
   Subtract
   {
        "srcVar1" "$curtime"
        "srcVar2" "$curtime3"
        "resultVar" "$secs"
   }

   Divide
   {
        "srcVar1" "$secs"
        "srcVar2" "$const2"
        "resultVar" "$secs2"
   }

   Int
   {
        "srcVar1" "$secs2"
        "resultVar" "$secs3"
   }

    Multiply
   {
        "srcVar1" "$secs3"
        "srcVar2" "$const2"
        "resultVar" "$secs4"
   }
    
   Subtract
   {
        "srcVar1" "$secs"
        "srcVar2" "$secs4"

UnlitGeneric
{
"$baseTexture" "vgui\replay/thumbnails/timer_hour\digits"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1
$curtime 0
$const 60
$curtime2 0
$curtime3 0
$frame 1
$mins 0
$mins2 0
$mins3 0
$mins4 0
$secs 0
$const2 10

Proxies
{
   CurrentTime
    {
        "resultVar" "$curtime"
    }
 
   Divide
   {
        "srcVar1" "$curtime"
        "srcVar2" "$const"
        "resultVar" "$curtime2"
   }

   Int
   {
        "srcVar1" "$curtime2"
        "resultVar" "$mins"
   }

   Divide
   {
        "srcVar1" "$mins"
        "srcVar2" "$const2"
        "resultVar" "$mins2"
   }

   Int
   {
        "srcVar1" "$mins2"
        "resultVar" "$mins3"
   }

    Multiply
   {
        "srcVar1" "$mins3"
        "srcVar2" "$const2"
        "resultVar" "$mins4"
   }
    
   Subtract
   {
        "srcVar1" "$mins"
        "srcVar2" "$mins4"
        "resultVar" "$frame"
   }
  
}

}

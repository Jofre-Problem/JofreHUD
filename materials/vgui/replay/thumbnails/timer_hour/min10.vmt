UnlitGeneric
{
"$baseTexture" "vgui\replay/thumbnails/timer_hour\digits"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1

$curtime 0
$const 60
$curtime2 0
$frame 1
$mins 0
$mins2 0
$const2 10
$const3 6
$a 0
$b 0
$c 0

Proxies
{
   CurrentTime
    {
        "resultVar" "$curtime"
    }

   Divide
   {
        "srcVar1" "$curtime"
        "srcVar2" "$const"
        "resultVar" "$curtime2"
   }

   Int
   {
        "srcVar1" "$curtime2"
        "resultVar" "$mins"
   }

   Divide
   {
        "srcVar1" "$mins"
        "srcVar2" "$const2"
        "resultVar" "$mins2"
   }
   
   Divide
   {
        "srcVar1" "$mins2"
        "srcVar2" "$const3"
        "resultVar" "$a"
   }
   
   Int
   {
        "srcVar1" "$a"
        "resultVar" "$b"
   }
   
   Multiply
   {
        "srcVar1" "$b"
        "srcVar2" "$const3"
        "resultVar" "$c"
   }
   
   Subtract
   {
        "srcVar1" "$mins2"
        "srcVar2" "$c"
        "resultVar" "$frame"
   }

}

}

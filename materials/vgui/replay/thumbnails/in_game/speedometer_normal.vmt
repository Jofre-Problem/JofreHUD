"UnlitGeneric"
{
    "$baseTexture"              "vgui/replay/thumbnails/in_game/speedometer_normal"
    "$translucent"              "1"
    "$vertexalpha"              "1"
    "$vertexcolor"              "1"
    "$additive"                 "1"

    "$playerSpeedRaw"           "0"
    "$playerSpeedMulti"         "0"
    "$playerSpeed"              "0"
    "$playerSpeedInt"           "0"

    "$imageWidth"               "512"
    "$playerSpeedMax"           "520"

    "Proxies"
    {
        "PlayerSpeed"
        {
            "scale"     "1"
            "resultVar" "$playerSpeedRaw"
        }
        "Clamp"
        {
            "min"       "0"
            "max"       "9999"
            "srcVar1"   "$playerSpeedRaw"
            "resultVar" "$playerSpeedRaw"
        }
        "Multiply"
        {
            "srcVar1"   "$playerSpeedRaw"
            "srcVar2"   "$imageWidth"
            "resultVar" "$playerSpeedMulti"
        }
        "Divide"
        {
            "srcVar1"   "$playerSpeedMulti"
            "srcVar2"   "$playerSpeedMax"
            "resultVar" "$playerSpeed"
        }
        "Int"
        {
            "srcVar1"   "$playerSpeed"
            "resultVar" "$frame"
        }
        "Clamp"
        {
            "min"       "0"
            "max"       "512"
            "srcVar1"   "$frame"
            "resultVar" "$frame"
        }
    }
}
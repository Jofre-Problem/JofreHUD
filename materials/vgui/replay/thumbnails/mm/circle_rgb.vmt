UnlitGeneric
{
	$baseTexture		"vgui/replay/thumbnails/mm/circle_rgb"
	$vertexcolor		1
	$no_fullbright		1
	$ignorez			1
	%keywords		"tf"
	$translucent		1
	$alpha			1
	$vertexalpha		1

"Proxies"
{
"AnimatedTexture" 
{ 
"animatedTextureVar" "$basetexture" 
"animatedTextureFrameNumVar" "$frame" 
"animatedTextureFrameRate" "4"
}
}
}
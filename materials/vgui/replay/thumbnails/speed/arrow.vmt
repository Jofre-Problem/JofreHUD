UnlitGeneric
{
$baseTexture "vgui\replay\thumbnails\speed/arrow"
$model 1
$playerspeed "0"
$translucent 1
$alpha 1
$vertexalpha 1
$ignorez 1
$no_fullbright 1
$vertexcolor 1

"$angle" 0
"$translate" "[0.0 0.0]"
"$center" "[0.5 0.5]"
"$scale" "[1 1]"
$const -1
$angle2 0
$const2 30

"Proxies" 
{
           "PlayerSpeed"
            {
                "scale" 0.2
                "resultVar" "$angle"
            }
            
           "Multiply"
            {
               "srcVar1" "$angle"
               "srcVar2" "$const"
               "resultVar" "$angle2"
            }
            
            "Clamp"
            {           
                "min" "-241"
                "max" "0"
                "srcVar1" "$angle2"
                "resultVar" "$angle2"
            }

		Add
		{
			srcVar1 "$angle2"
			srcVar2 "$const2"
			resultVar "$angle2"
		}
          
            "TextureTransform"
            {
                "translateVar" "$translate"
                "rotateVar" "$angle2"
                "centerVar" "$center"
                "resultVar" "$basetexturetransform"
            }


}

}